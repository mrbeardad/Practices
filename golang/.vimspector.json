{
    "$schema": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json",
    "configurations": {
        "debug Go": {
            "adapter": "vscode-go",
            "configuration": {
                "request": "launch",
                "program": "${file}",
                "mode": "debug",
                "dlvToolPath": "/bin/dlv",
                "trace": true,
                "stopAtEntry": true,
                "stopOnEntry": true,
                "env": { "GO111MODULE": "off" }
            }
        }
    }
}
